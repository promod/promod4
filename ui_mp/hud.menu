#define ENFORCE( condition, txt, position ) \
        itemDef \
        { \
            rect        0 position 1 1 HORIZONTAL_ALIGN_CENTER VERTICAL_ALIGN_CENTER \
            origin      107 0 \
            forecolor   1 0 0 1 \
            backcolor   1 0 0 1 \
            exp         text( txt ); \
